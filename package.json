  "devDependencies": {
    "@babel/highlight": "^7.25.9",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "gh-pages": "^6.3.0",
